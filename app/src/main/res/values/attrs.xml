<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="Responsive">
        <attr name="collapseScale" format="float" />
        <attr name="collapseOffsetX" format="integer"/>
        <attr name="collapseOffsetY" format="integer"/>
    </declare-styleable>

    <declare-styleable name="ResponsiveTextView">
        <attr name="textColor" format="string" />
        <attr name="textSize" format="integer" />
        <attr name="textAlignCenter" format="boolean" />
    </declare-styleable>

    <declare-styleable name="Scroll">
        <attr name="scrollStart" format="float" />
        <attr name="scrollEnd" format="float" />
    </declare-styleable>

    <declare-styleable name="Alpha">
        <attr name="alphaMin" format="float" />
        <attr name="alphaMax" format="float" />
    </declare-styleable>

    <declare-styleable name="Blur">
        <attr name="blurRadius" format="float" />
        <attr name="blurRepeat" format="integer" />
    </declare-styleable>

    <declare-styleable name="Icon">
        <attr name="iconColor" format="string" />
    </declare-styleable>
</resources>