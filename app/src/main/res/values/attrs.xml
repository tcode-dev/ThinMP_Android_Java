<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="Responsive">
        <attr name="collapseScale" format="float" />
        <attr name="collapseOffsetX" format="integer"/>
        <attr name="collapseOffsetY" format="integer"/>
    </declare-styleable>

    <declare-styleable name="ResponsiveTextView">
        <attr name="textColor" format="string" />
        <attr name="textSize" format="integer" />
    </declare-styleable>

    <declare-styleable name="Fade">
        <attr name="fadeStart" format="float" />
        <attr name="fadeEnd" format="float" />
        <attr name="fadeIn" format="boolean" />
    </declare-styleable>

    <declare-styleable name="Blur">
        <attr name="blurRadius" format="float" />
        <attr name="blurRepeat" format="integer" />
    </declare-styleable>
</resources>